#include <stdio.h>
#include <math.h>

void main() {
    int popularity;
    int sizetype;

    printf("Enter popularity: ");
    scanf("%d", &popularity);

    printf("Enter size: ");
    scanf("%d", &sizetype);

    int homeValue = (pow(popularity, 3) + pow(sizetype, 2)) * 10000;
    printf("Home value is: %d\n", homeValue);
}
